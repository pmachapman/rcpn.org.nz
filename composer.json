{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"license" : "proprietary",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "^3.4",
		"silverstripe/framework": "^3.4",
		"silverstripe/reports": "^3.4",
		"silverstripe/siteconfig": "^3.4",
		"silverstripe-themes/simple": "^3.1",
		"silverstripe/googlesitemaps": "^1.5",
		"silverstripe/timepickerfield": "^1.0",
		"symbiote/silverstripe-gridfieldextensions": "^1.4",
		"undefinedoffset/silverstripe-nocaptcha": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~3.7"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.4.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
